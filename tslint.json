{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "quotemark":[true,"single"],
        "arrow-parens": true,
        "max-line-length": [true, 140],
        "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
    },
    "rulesDirectory": []
}